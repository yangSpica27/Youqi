<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FunctionItem">
        <attr name="src" format="reference" />
        <attr name="title" format="string" />
        <attr name="subtitle" format="string" />
        <attr name="image_bg" format="reference" />
    </declare-styleable>


    <declare-styleable name="SettingItem">
        <attr name="image_src" format="reference" />
        <attr name="name" format="string" />
    </declare-styleable>


    <!-- 自定义的textView-->
    <declare-styleable name="SpicaTextView">
        <attr name="mouth" format="string" />
        <attr name="day" format="string" />
        <attr name="textColor" format="color" />
        <attr name="lineColor" format="color" />
    </declare-styleable>

    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>

    <declare-styleable name="MarkdownStylesBar">
        <attr name="buttonColor" format="color"/>
    </declare-styleable>


    <declare-styleable name="KeyboardLayout">
        <attr name="input_aera_id" format="reference"/>
    </declare-styleable>

</resources>